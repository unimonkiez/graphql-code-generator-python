  "bugs": {
    "url": "https://github.com/unimonkiez/graphql-code-generator-python/issues"
  },
  "homepage": "https://github.com/unimonkiez/graphql-code-generator-python#readme",
  "devDependencies": {
    "tsc-watch": "^4.2.9",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
